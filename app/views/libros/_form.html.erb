<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <%= form_for(@libro) do |f| %>
        <% if @libro.errors.any? %>
          <div id="error_explanation">
            <h2><%= pluralize(@libro.errors.count, "error") %> prohibited this libro from being saved:</h2>

            <ul>
            <% @libro.errors.full_messages.each do |message| %>
              <li><%= message %></li>
            <% end %>
            </ul>
          </div>
        <% end %>

        <div class="field">
          <strong>Numero de Inscripcion</strong><br>
          <%= f.text_field :numInsc, autofocus: true, class: "form-control" %>
        </div>
        <div class="field">
          <%= f.label :TitLib %><br>
          <%= f.text_field :TitLib, autofocus: true, class: "form-control" %>
        </div>
        <div class="field">
          <%= f.label :AutLib %><br>
          <%= f.text_field :AutLib, autofocus: true, class: "form-control" %>
        </div>
        <div class="field">
          <%= f.label :EdicLib %><br>
          <%= f.text_field :EdicLib, autofocus: true, class: "form-control" %>
        </div>
        <div class="field">
          <strong>Editorial</strong> <br>
          <%= collection_select(:libro, :editorial_id, Editorial.all, :id, :NomEdi, prompt: true) %>
          <%= link_to "Agregar nueva editorial", new_editorial_path %>
        </div>
        <div class="field">
          <%= f.label :AnioLib %><br>
          <%= f.number_field :AnioLib, autofocus: true, class: "form-control" %>
        </div>
        <div class="field">
          <%= f.label :FecLib %><br>
          <%= f.datetime_select :FecLib, autofocus: true, class: "form-control" %>
        </div>
        <div class="field">
          <%= f.label :LugLib %><br>
          <%= f.text_field :LugLib, autofocus: true, class: "form-control" %>
        </div>
        <div class="field">
          <%= f.label :ObsLib %><br>
          <%= f.text_area :ObsLib, autofocus: true, class: "form-control" %>
        </div>
        <div class="field">
          <%= f.label :DesLib %><br>
          <%= f.text_area :DesLib, autofocus: true, class: "form-control" %>
        </div>
        <div class="field">
          <%= f.label :CantLib %><br>
          <%= f.number_field :CantLib, autofocus: true, class: "form-control" %>
        </div>
        <div class="field">
          <%= f.label :Disp %><br>
          <%= f.number_field :Disp, autofocus: true, class: "form-control" %>
        </div>
        <div class="field">
          <%= f.label :CodDewLib %><br>
          <%= f.text_field :CodDewLib, autofocus: true, class: "form-control" %>
        </div><br>
        <div class="text-center">
                <%= f.submit "Aceptar", class: "btn btn-primary" %>
        </div>
      <% end %>
    </div>
  </div>
</div>
<br>
<br>
<br>
<br>
<br>
